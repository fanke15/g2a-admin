{
  "type": "app",
  "brandName": "G2aAdmin",
  "header": {
    "type": "flex",
    "className": "w-full",
    "justify": "flex-end",
    "alignItems": "flex-end",
    "items": [
      {
        "type": "dropdown-button",
        "icon": "fa fa-user",
        "label": "admin",
        "trigger": "hover",
        "buttons": [
          {
            "type": "button",
            "actionType": "dialog",
            "label": "用户信息",
            "dialog": {
              "type": "dialog",
              "body": {
                "type": "service",
                "schemaApi": {
                  "url": "",
                  "method": "post",
                  "cache": 600000,
                  "responseData": {
                    "&": "${body}"
                  }
                }
              },
              "size": "lg",
              "actions": [],
              "title": "用户信息"
            }
          },
          {
            "type": "button",
            "actionType": "url",
            "label": "退出登录",
            "url": "/"
          }
        ]
      }
    ]
  },
  "pages": [
    {
      "children": [
        {
          "label": "Dashboard",
          "icon": "fa fa-home",
          "url": "/",
          "schemaApi": "get:/api/config/dashboard/chart",
          "isDefaultPage": true
        },
        {
          "label": "Account",
          "icon": "fa fa-lock",
          "url": "/account",
          "rewrite": "/account/info",
          "children": [
            {
              "label": "账户信息管理",
              "icon": "fa fa-user",
              "url": "info",
              "schemaApi": "get:/api/config/account/info"
            },
            {
              "label": "账户资产管理",
              "icon": "fa fa-group",
              "url": "asset",
              "schemaApi": "get:/api/config/account/asset"
            }
          ]
        }
      ]
    }
  ]
}